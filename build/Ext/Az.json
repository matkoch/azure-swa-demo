{
  "$schema": "https://raw.githubusercontent.com/nuke-build/nuke/master/source/Nuke.CodeGeneration/schema.json",
  "name": "Az",
  "officialUrl": "",
  "pathExecutable": "az",
  "tasks": [
    {
      "postfix": "StaticWebAppUsersInvite",
      "returnType": "string",
      "preProcess": true,
      "definiteArgument": "staticwebapp users invite",
      "settingsClass": {
        "properties": [
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}"
          },
          {
            "name": "AuthenticationProvider",
            "type": "AzAuthenticationProvider",
            "format": "--authentication-provider {value}"
          },
          {
            "name": "Roles",
            "type": "List<string>",
            "format": "--roles {value}",
            "separator": ","
          },
          {
            "name": "UserDetails",
            "type": "string",
            "format": "--user-details {value}"
          },
          {
            "name": "Domain",
            "type": "string",
            "format": "--domain {value}"
          },
          {
            "name": "InvitationExpirationInHours",
            "type": "int",
            "format": "--invitation-expiration-in-hours {value}"
          }
        ]
      }
    },
    {
      "postfix": "DeploymentGroupCreate",
      "preProcess": true,
      "definiteArgument": "deployment group create",
      "settingsClass": {
        "properties": [
          {
            "name": "ResourceGroup",
            "type": "string",
            "format": "--resource-group {value}"
          },
          {
            "name": "TemplateFile",
            "type": "string",
            "format": "--template-file {value}"
          },
          {
            "name": "Parameters",
            "type": "Dictionary<string, string>",
            "format": "--parameters {value}",
            "itemFormat": "{key}={value}",
            "separator": " "
          }
        ]
      }
    },
    {
      "postfix": "StaticWebAppHostnameSet",
      "preProcess": true,
      "definiteArgument": "staticwebapp hostname set",
      "settingsClass": {
        "properties": [
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}"
          },
          {
            "name": "ResourceGroup",
            "type": "string",
            "format": "--resource-group {value}"
          },
          {
            "name": "Hostname",
            "type": "string",
            "format": "--hostname {value}"
          },
          {
            "name": "ValidationMethod",
            "type": "AzValidationMethod",
            "format": "--validation-method {value}"
          }
        ]
      }
    },
    {
      "postfix": "StaticWebAppHostnameShow",
      "preProcess": true,
      "definiteArgument": "staticwebapp hostname show",
      "settingsClass": {
        "properties": [
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}"
          },
          {
            "name": "ResourceGroup",
            "type": "string",
            "format": "--resource-group {value}"
          },
          {
            "name": "Hostname",
            "type": "string",
            "format": "--hostname {value}"
          },
          {
            "name": "Query",
            "type": "string",
            "format": "--query {value}"
          }
        ]
      }
    }
  ],
  "enumerations": [
    {
      "name": "AzAuthenticationProvider",
      "values": [
        "AAD",
        "Facebook",
        "GitHub",
        "Google",
        "Twitter"
      ]
    },
    {
      "name": "AzValidationMethod",
      "values": [
        "dns-text-token"
      ]
    }
  ]
}